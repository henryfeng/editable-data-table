{
  "name": "@ticatec/uniface-editable-data-table",
  "version": "0.1.4",
  "description": "A customizable editable table component for Svelte supporting text, number, date, dropdown, and user-defined column types.",  "main": "index.js",
  "scripts": {
    "dev": "vite dev",
    "build:css": "sass src/lib/uniface-editable-datatable.scss:src/lib/uniface-editable-datatable.css --no-source-map",
    "build": "npm run build:css && svelte-kit sync && svelte-package",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "publish:public": "npm run build && npm publish --access public"
  },
  "files": [
    "dist",
    "!dist/**/*.scss",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "keywords": [
    "svelte",
    "editable-table",
    "table",
    "custom-types",
    "data-grid",
    "dropdown",
    "date-picker"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./uniface-editable-datatable.css": {
      "default": "./dist/uniface-editable-datatable.css"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@ticatec/app-data-manager": "^1.0.7",
    "@ticatec/app-data-service": "^1.0.3",
    "@ticatec/axios-restful-service": "^0.1.6",
    "@ticatec/enhanced-utils": "^0.9.0",
    "@ticatec/i18n": "^0.0.8",
    "@ticatec/uniface-dev-shell": "^0.0.6",
    "@ticatec/uniface-element": "^0.1.62",
    "@ticatec/uniface-google-material-icons": "^0.1.2",
    "@ticatec/web-bean-validator": "^0.0.12",
    "@ticatec/uniface-icons": "^0.2.0",
    "axios": "^1.8.2",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.7",
    "publint": "^0.3.2",
    "sass": "^1.57.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-portal": "^2.2.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.3.1",
    "typescript": "^5.7.3",
    "vite": "^5.4.11"
  },
  "author": "Henry Feng",
  "license": "MIT",
  "private": false,
  "dependencies": {
  }
}
